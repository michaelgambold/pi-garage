{
  "name": "pi-garage",
  "version": "1.8.2",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "docs": "docsify serve ./docs",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migration:create": "npx mikro-orm migration:create",
    "migration:up": "npx mikro-orm migration:up",
    "seeder:run": "npx mikro-orm seeder:run",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@mikro-orm/core": "^5.6.11",
    "@mikro-orm/migrations": "^5.6.11",
    "@mikro-orm/nestjs": "^5.1.7",
    "@mikro-orm/reflection": "^5.6.11",
    "@mikro-orm/seeder": "^5.6.11",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "@mikro-orm/sqlite": "^5.6.11",
    "@nestjs/common": "^9.1.2",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.1.2",
    "@nestjs/platform-express": "^9.4.2",
    "@nestjs/platform-socket.io": "^9.3.9",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/websockets": "^9.1.4",
    "automation-hat": "^1.0.2",
    "date-fns": "^2.29.3",
    "delay": "^5.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.5.7",
    "semver": "^7.3.8",
    "socket.io": "^4.5.3"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^5.6.11",
    "@nestjs/cli": "^9.1.4",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.1.4",
    "@types/express": "^4.17.17",
    "@types/jest": "^27.4.1",
    "@types/node": "^20.2.1",
    "@types/semver": "^7.3.13",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  }
}
